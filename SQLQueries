
DROP TABLE user;

CREATE TABLE user (
    id INTEGER NOT NULL AUTO_INCREMENT,
    name VARCHAR(32) NOT NULL,
    email VARCHAR(64) NOT NULL,
    gender INTEGER,
    dob DATE,
    country VARCHAR(32),
    PRIMARY KEY(id)
);

